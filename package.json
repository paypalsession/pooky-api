{
  "name": "pooky-api",
  "description": "A GraphQL + Express Boilerplate for a dynamic Pooky API",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "babel --root-mode upward --no-comments -d dist src/",
    "start": "yarn dev",
    "dev": "cross-env NODE_ENV=development yarn run dev:nodemon",
    "dev:nodemon": "nodemon --config nodemon.json",
    "dev:node": "node dist/server.js",
    "heroku-postbuild": "yarn --production=false && yarn run build",
    "postinstall": "yarn run build",
    "lint": "eslint --ext js src/",
    "lint:fix": "eslint --fix --ext js src/",
    "clean": "concurrently \"yarn clean:node_modules\" \"yarn clean:dist\"",
    "clean:node_modules": "cross-env rm -rf node_modules && rm -rf packages/**/node_modules",
    "clean:dist": "cross-env rm -rf packages/**/dist",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "concurrently": "5.0.0",
    "cors": "^2.8.5",
    "cross-env": "6.0.3",
    "dotenv": "^7.0.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.2.1",
    "graphql-type-json": "^0.3.0",
    "ioredis": "^4.14.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "yarn": "^1.12.3"
  }
}
